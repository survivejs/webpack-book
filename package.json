  },
  "homepage": "https://github.com/survivejs/webpack-book",
  "dependencies": {},
  "devDependencies": {
    "check-markdown-links": "^0.1.1",
    "git-prepush-hook": "^1.0.2",
    "leanpub-cli": "^0.2.0",
    "proselint": "^0.1.7",
    "textlint": "^7.3.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-terminology": "^1.1.6",
    "textlint-rule-write-good": "^1.6.0"
